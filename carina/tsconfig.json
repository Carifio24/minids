  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ]
  },
  "include": [
    "../components/*.ts",
    "../components/*.vue",
